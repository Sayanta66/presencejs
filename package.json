  "module": "dist/yomo-js.esm.js",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.3",
    "husky": "^7.0.4",
    "jest-fetch-mock": "^3.0.3",
    "np": "^7.6.0",
    "size-limit": "^7.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "rxjs": "^6.6.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yomorun/yomo-js.git"
  },
